{
  "name": "monster-counter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev": "eas build --profile development --platform android",
    "build:preview": "eas build --profile preview --platform android",
    "build:prod": "eas build --profile production --platform android",
    "submit": "eas submit --platform android",
    "prebuild": "expo prebuild --clean",
    "typecheck": "tsc --noEmit",
    "clean": "expo prebuild --clean && cd android && ./gradlew clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-navigation/bottom-tabs": "7.0.0",
    "@react-navigation/native": "7.0.0",
    "@supabase/supabase-js": "^2.96.0",
    "babel-preset-expo": "~54.0.10",
    "expo": "~54.0.33",
    "expo-auth-session": "~7.0.10",
    "expo-crypto": "~15.0.8",
    "expo-localization": "~17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.8.13",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@types/react-native": "^0.72.8",
    "typescript": "~5.9.2"
  },
  "private": true
}
